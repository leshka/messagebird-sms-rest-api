{
    "name": "messagebird/sms-rest-api",
    "type": "project",
    "authors": [
        {
            "name": "Akulshyn Oleksii",
            "email": "greeni@coccoc.vn"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "messagebird/php-rest-api": "^1.9",
        "psr/container": "^1.0",
        "psr/log": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan": "^0.9.2",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "RestApi\\": "src/RestApi/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestApi\\Tests\\": "tests/RestApi/"
        }
    }
}
